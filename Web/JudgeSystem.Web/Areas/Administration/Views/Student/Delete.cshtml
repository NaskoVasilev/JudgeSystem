@model StudentProfileViewModel

@{
    ViewData["Title"] = "Delete Student";
}

<h1>@ViewData["Title"]</h1>
<div class="row">
	<div class="col-md-4" >
		<section>
			<form method="post" enctype="multipart/form-data">
				<div class="form-group">
					<label asp-for="FullName"></label>
					<input asp-for="FullName" class="form-control" disabled />
				</div>
				<div class="form-group">
					<label asp-for="Email"></label>
					<input asp-for="Email" class="form-control" disabled />
				</div>
				<div class="form-group">
					<label asp-for="NumberInCalss"></label>
					<input asp-for="NumberInCalss" class="form-control" disabled />
				</div>
				<div class="form-group">
					<label asp-for="SchoolClassName"></label>
					<input asp-for="SchoolClassName" class="form-control" disabled />
				</div>
				<div class="form-group">
					<button type="submit" class="btn btn-primary">Delete</button>
				</div>
			</form>
		</section>
	</div>
</div>
