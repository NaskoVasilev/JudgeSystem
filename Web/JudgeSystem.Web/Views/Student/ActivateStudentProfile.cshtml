@model StudentActivateProfileInputModel

@{
	ViewData["Title"] = "Activate Student Profile";
}

<h4>@ViewData["Title"]</h4>
<div class="row">
	<div class="col-md-6">
		<form id="change-password-form" method="post">
			<div asp-validation-summary="All" class="text-danger"></div>
			<div class="form-group">
				<label asp-for="ActivationKey">Activation Key</label>
				<input asp-for="ActivationKey" class="form-control" />
				<span asp-validation-for="ActivationKey" class="text-danger"></span>
			</div>
			<button type="submit" class="btn btn-primary">Actvate</button>
		</form>
	</div>
</div>

